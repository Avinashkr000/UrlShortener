# React App Environment Variables
# Copy this file to .env.local and update the values

# Backend API URL - Updated with Production URL
REACT_APP_API_URL=https://url-shortener-backend-k0pv.onrender.com

# App Configuration
REACT_APP_NAME="URL Shortener"
REACT_APP_VERSION=1.0.0
REACT_APP_DESCRIPTION="Modern URL Shortener with Analytics"

# App URLs
REACT_APP_FRONTEND_URL=http://localhost:3000
REACT_APP_BACKEND_URL=https://url-shortener-backend-k0pv.onrender.com

# Analytics (Optional)
REACT_APP_GA_TRACKING_ID=

# Development API URL (for local development)
# REACT_APP_API_URL=http://localhost:8080

# Features Toggle
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_QR_CODE=true
REACT_APP_ENABLE_EXPIRY=true
